.explore-music-wrapper
  position: relative
  overflow: hidden
  white-space: nowrap
  width: 100%
  height: 100%
  box-sizing: border-box
  .explore-music
    position: relative
    box-sizing: border-box
    width: 100%
    padding-bottom: 15px
    .lala
      position: relative
      width: 90%
      height: 250px
      margin: 15px auto
      overflow: hidden
    .recommend-song-list
      .recommend-song-list-content
        display: flex
        justify-content: space-between
        flex-wrap: wrap
        align-content: space-between
        width: 80%
        margin: 0 auto
        .recommend-song-list-content-item
          flex: 1
          margin-bottom: 5px
    .latest-music
      .latest-music-content
        display: flex
        flex-wrap: wrap
        justify-content: space-between
        width: 100%
        .latest-music-content-item
          display: inline-block
          box-sizing: border-box
          width: 50%
          &:nth-child(odd)
            border-right: 1px solid #d9d9d9
          &:nth-child(1)
            background: #f5f5f5
          &:nth-child(2)
            background: #f5f5f5
          &:nth-child(5)
            background: #f5f5f5
          &:nth-child(6)
            background: #f5f5f5
          &:nth-child(9)
            background: #f5f5f5
          &:nth-child(10)
            background: #f5f5f5