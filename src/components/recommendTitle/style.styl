.recommend-title-wrapper
  position: relative
  width: 100
  height: 35px
  border-bottom: 1px solid #bfbfbf
  margin-bottom: 10px
  .recommend-title-wrapper-left
    position: absolute
    left: 0
    top: 0
    .recommend-title-wrapper-left-text
      height: 30px
      line-height: 30px
      font-size: 16px
      font-weight: 600
      text-align: center
  .recommend-title-wrapper-right
    position: absolute
    right: 0
    top: 0
    .recommend-title-wrapper-right-text
      height: 30px
      line-height: 30px
      font-size: 12px
      color: #5a5a5a
      text-align: center